# Copy to .env and adjust for your setup.
# Only variables prefixed with VITE_ are exposed to the frontend (Vite).

# Base URL for the backend REST API (auth, meetings, etc.).
# Leave empty if using Vite proxy to the same origin (e.g. proxy /api to backend).
# Example for separate backend: http://localhost:5000
VITE_API_URL=

# WebSocket URL for Socket.IO (live transcription).
# Leave empty to use same origin as the app (works with Vite proxy).
# Example for separate backend: http://localhost:5000
VITE_SOCKET_URL=
